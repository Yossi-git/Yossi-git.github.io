  	<span class="hljs-keyword">while</span> (<span class="hljs-keyword">this</span>.loop) &#123;
    	clickAllEl(<span class="hljs-string">'.del_btn'</span>, iframeDocument)
    	<span class="hljs-keyword">await</span> sleepAsync(<span class="hljs-number">500</span>) 			<span class="hljs-comment">//点击删除，延迟500ms</span>
